  "type": "library",
  "require": {
    "php": "^7.0",
    "darrynten/any-cache": "^1.0",
    "guzzle/guzzle": "^3.9",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "dev-master",
    "internations/http-mock": "^0.10.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
